    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "sensiolabs/security-checker": "^6.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "remove-git-submodules": "find . -mindepth 2 -type d -name .git | xargs rm -rf",
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "@remove-git-submodules"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "@remove-git-submodules"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": false,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/paragraphs": {
                "Integrity constraint violation: 1048 Column 'langcode' cannot be null": "https://www.drupal.org/files/issues/2019-08-10/paragraphs-set_langcode_widgets-290139_updated.patch",
                "Paragraphs do not render: access check for 'view' fail when using layout builder": "https://www.drupal.org/files/issues/2020-07-08/access-controll-issue-3090200-22.patch"
            },
            "drupal/core": {
                "Undefined Index: Region": "https://www.drupal.org/files/issues/2019-05-10/more_strict_check-3053906-1.patch",
                "Creating a block that uses Ajax adds multiple blocks": "https://www.drupal.org/files/issues/2019-05-04/3042190-3.patch",
                "Error: Call to a member function getEntityTypeId() on null (Layout Builder)": "https://www.drupal.org/files/issues/2019-06-25/drupal-layout_builder_inline_block_empty_block_error-3049332-8.7.x_0.patch",
                "Cloned entities don't retain their Inline Block usage": "https://www.drupal.org/files/issues/2020-03-06/3062819-inline-cloned-COMBINED_2.patch",
                "Conflict with field_group, missing options in sidebar when creating/updating a node": "https://www.drupal.org/files/issues/2019-11-29/claro-fix_node_sidebar_accordion_items-3097407-4--fix-only.patch",
                "Break long text strings in layout edit": "https://www.drupal.org/files/issues/2020-08-10/break-long-text-strings-in-layout-3016507-32.patch",
                "Views file name suggestions often missing from Twig debugging": "https://www.drupal.org/files/issues/views-template-suggestions-2839945-2.patch",
                "'Negate' form value for condition plugins should be cast to boolean in validation.": "https://www.drupal.org/files/issues/2021-01-06/3114467-16.patch",
                "Support third party settings for components within a section": "https://www.drupal.org/files/issues/2021-12-13/3015152-tps-9.3.x-128.patch",
                "/project/drupal/issues/3056633 - Drupal 9 updating issue plugin does not exist": "https://www.drupal.org/files/issues/2022-04-05/3056633-147.patch"
            },
            "drupal/block_class": {
                "Integration with Drupal core's new Layout Builder": "https://www.drupal.org/files/issues/2019-06-28/block_class-support-layout-builder-2998114-45.patch"
            },
            "drupal/metatag": {
                "Add a TypedData plugin to support Search API": "https://www.drupal.org/files/issues/2019-06-14/2901039-32.patch"
            },
            "drupal/commerce_smart_importer": {
                "Blank URL parameter import_name after uploading csv": "https://www.drupal.org/files/issues/2020-04-29/3115808.patch"
            },
            "drupal/commerce_square": {
                "Zip Code not valid": "https://www.drupal.org/files/issues/2020-03-30/3123602-2.patch"
            },
            "drupal/commerce": {
                "Promotion without coupons is always applied": "https://www.drupal.org/files/issues/promotion_without-2884888-12.patch"
            },
            "drupal/jquery_ui_touch_punch": {
                "Download the library": "https://www.drupal.org/files/issues/2020-11-19/3159222-32.patch"
            },
            "drupal/simple_image_rotate": {
                "Undefined methond getThirdPartySettings()": "https://www.drupal.org/files/issues/2020-08-26/simple-image-rotate-issue-3126067-undefined-getThirdPartySettings-method.patch"
            },
            "drupal/better_exposed_filters": {
                "BEF Links not working with AJAX": "https://www.drupal.org/files/issues/2020-12-11/better_exposed_filters_2938761-49.patch"
            },
            "drupal/colorbox": {
                "Allow media entity reference fields to be displayed as a gallery in a colorbox": "https://www.drupal.org/files/issues/2020-04-24/2957030-27.patch"
            },
            "drupal/commerce_giftcard": {
                "Theme hook commerce_giftcard_redemption_form not found": "https://www.drupal.org/files/issues/2020-11-06/theme_hook_commerce_giftcard_redemption_form_not_found-3180964-3.patch",
                "Gift cards not created when other items exist in the checkout before the gift card item": "https://git.drupalcode.org/project/commerce_giftcard/-/merge_requests/4.diff",
                "Event for new giftcard purchase": "https://git.drupalcode.org/project/commerce_giftcard/-/merge_requests/2.diff",
                "Don't allow payout on certain items": "./patches/commerce_giftcard_custom_patch_no_payout_allowed.patch"
            },
            "drupal/commerce_pos": {
                "Auto set register to id of 1": "./patches/load-only-register-1b.patch"
            },
            "drupal/commerce_email": {
                "Log emails into order activity log": "https://www.drupal.org/files/issues/2020-12-24/commerce_email-log-emails-to-order-activity-log-3189770-2.patch"
            },
            "drupal/feeds": {
                "Undefined Index: allowed_extensions AJAX error when creating feed types.": "https://www.drupal.org/files/issues/2019-02-06/3031019-undefined-index-ajax-2.patch"
            },
            "drupal/rules": {
                "/project/rules/issues/2471481 - Integrate Typed Data Widgets": "https://www.drupal.org/files/issues/2022-02-15/2471481-170.integrate-typed-data-widgets-no-tests.patch"
            },
            "drupal/easy_responsive_images": {
                "Fix retina display issues": "./patches/easy_responsive_images_fix_retina_display_issues.patch"
            }
        },
        "enable-patching": true,
        "preserve-paths": [],
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false
            }
        }
    }
