{
    "name": "oomphinc/drupal-scaffold",
    "description": "A Drupal 8 scaffold project",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Oomph, Inc.",
            "email": "devops@oomphinc.com",
            "homepage": "https://www.oomphinc.com/"
        },
        {
            "name": "Nathan Dentzau",
            "email": "nathan.dentzau@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/font",
                "version": "v4.13.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/font/releases/font_4.13.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/colorbutton",
                "version": "v4.13.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.13.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/panelbutton",
                "version": "v4.13.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.13.1.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "ext-json": "^1.6",
        "bower-asset/bootstrap": "^4.0",
        "bower-asset/clipboard": "^2.0",
        "bower-asset/noty": "^3.1",
        "bower-asset/slick-lightbox": "^0.2.12",
        "ckeditor/colorbutton": "^4.13",
        "ckeditor/font": "^4.13",
        "ckeditor/panelbutton": "^4.13",
        "composer/installers": "^1.5",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.4",
        "drupal-composer/preserve-paths": "^0.1.4",
        "drupal/admin_toolbar": "^2.0",
        "drupal/block_class": "^1.1",
        "drupal/ckeditor_font": "^1.0",
        "drupal/coder": "^8.3",
        "drupal/coffee": "^1.0",
        "drupal/color_field": "^2.2",
        "drupal/commerce": "^2.20",
        "drupal/commerce_cart_flyout": "^1.8",
        "drupal/commerce_square": "^1.3",
        "drupal/config_ignore": "^2.2",
        "drupal/config_installer": "^1.8",
        "drupal/config_split": "^1.3",
        "drupal/core": "^8.6",
        "drupal/ctools": "^3.2",
        "drupal/devel": "^4.0",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/extlink": "^1.2",
        "drupal/field_group": "^3.1",
        "drupal/flexi_cart": "^1.2",
        "drupal/fontawesome": "^2.17",
        "drupal/google_tag": "^1.3",
        "drupal/honeypot": "^1.27",
        "drupal/jquery_ui_accordion": "^1.0",
        "drupal/jquery_ui_tooltip": "^1.0",
        "drupal/layout_builder_modal": "^1.0",
        "drupal/layout_builder_restrictions": "^2.5",
        "drupal/media_library_form_element": "^1.0@alpha",
        "drupal/media_library_theme_reset": "^1.0",
        "drupal/metatag": "^1.13",
        "drupal/paragraphs": "^1.11",
        "drupal/pathauto": "^1.6",
        "drupal/quick_node_clone": "^1.12",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recreate_block_content": "^2.0",
        "drupal/redirect": "^1.5",
        "drupal/responsive_favicons": "^1.5",
        "drupal/search_api": "^1.15",
        "drupal/simple_instagram_feed": "^3.9",
        "drupal/simple_sitemap": "^3.5",
        "drupal/sitewide_alert": "^1.3",
        "drupal/twig_tweak": "^2.6",
        "drupal/upgrade_status": "^2.9",
        "drupal/view_unpublished": "1.x-dev",
        "drupal/views_ajax_history": "^1.2",
        "drupal/viewsreference": "^1.4",
        "drush/drush": "^9.2",
        "npm-asset/bootstrap": "^4.5",
        "npm-asset/slick-carousel": "^1.8",
        "oomphinc/composer-installers-extender": "^1.1",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "sensiolabs/security-checker": "^6.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ]
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/paragraphs": {
                "Integrity constraint violation: 1048 Column 'langcode' cannot be null": "https://www.drupal.org/files/issues/2019-08-10/paragraphs-set_langcode_widgets-290139_updated.patch",
                "Paragraphs do not render: access check for 'view' fail when using layout builder": "https://www.drupal.org/files/issues/2020-07-08/access-controll-issue-3090200-22.patch"
            },
            "drupal/core": {
                "Undefined Index: Region": "https://www.drupal.org/files/issues/2019-05-10/more_strict_check-3053906-1.patch",
                "Creating a block that uses Ajax adds multiple blocks": "https://www.drupal.org/files/issues/2019-05-04/3042190-3.patch",
                "Error: Call to a member function getEntityTypeId() on null (Layout Builder)": "https://www.drupal.org/files/issues/2019-06-25/drupal-layout_builder_inline_block_empty_block_error-3049332-8.7.x_0.patch",
                "Cloned entities don't retain their Inline Block usage": "https://www.drupal.org/files/issues/2020-03-06/3062819-inline-cloned-COMBINED_2.patch",
                "Support third party settings for components within a section": "https://www.drupal.org/files/issues/2019-12-05/3015152-tps-26.patch",
                "Add a hook to modify oEmbed resource data": "https://www.drupal.org/files/issues/2019-03-25/media_hook-oembed-resource-data-alter__example.patch",
                "Conflict with field_group, missing options in sidebar when creating/updating a node": "https://www.drupal.org/files/issues/2019-11-29/claro-fix_node_sidebar_accordion_items-3097407-4--fix-only.patch",
                "Layout builder incorrectly resolves global contexts values when viewing layouts": "https://www.drupal.org/files/issues/2020-06-19/3099968-35.patch",
                "Break long text strings in layout edit": "https://www.drupal.org/files/issues/2020-08-10/break-long-text-strings-in-layout-3016507-32.patch",
                "Nested Paragraphs create multiple drag handles": "https://www.drupal.org/files/issues/2019-11-13/claro-nested_paragraphs_multiple_drag_handles-3092181-5.patch"
            },
            "drupal/block_class": {
                "Integration with Drupal core's new Layout Builder": "https://www.drupal.org/files/issues/2019-06-28/block_class-support-layout-builder-2998114-45.patch"
            },
            "drupal/metatag": {
                "Add a TypedData plugin to support Search API": "https://www.drupal.org/files/issues/2019-06-14/2901039-32.patch"
            }
        },
        "enable-patching": true,
        "preserve-paths": [],
        "drupal-scaffold": {}
    }
}
